#ifndef OBJET_H
#define OBJET_H

#include <string>


class Objet
{
    public:
        Objet();
        virtual ~Objet();
        std::string getName(){return this->name;};
        void setName(std::string name);


    protected:

    private:
        std::string name;
};

#endif // OBJET_H
